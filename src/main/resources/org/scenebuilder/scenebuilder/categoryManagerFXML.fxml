<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.scenebuilder.scenebuilder.CategoryManagerFXMLController">
   <children>
      <Button layoutX="14.0" layoutY="746.0" mnemonicParsing="false" onAction="#backFromCategoryManager" prefHeight="39.0" prefWidth="73.0" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="95.0" layoutY="746.0" mnemonicParsing="false" onAction="#saveFromCategoryManager" prefHeight="39.0" prefWidth="73.0" text="Save">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="35.0" prefWidth="121.0" text="Categories">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ScrollPane layoutX="14.0" layoutY="120.0" prefHeight="612.0" prefWidth="493.0" style="-fx-background-color: transparent;">
         <content>
            <VBox fx:id="categoriesVBox" prefHeight="605.0" prefWidth="484.0" />
         </content>
      </ScrollPane>
      <Button fx:id="removeCategoryButton" disable="true" layoutX="300.0" layoutY="746.0" mnemonicParsing="false" onAction="#removeCategory" prefHeight="39.0" prefWidth="202.0" text="Remove Category">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TextField fx:id="categoryTextField" layoutX="14.0" layoutY="55.0" prefHeight="39.0" prefWidth="262.0" promptText="New Category">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="addCategoryButton" layoutX="305.0" layoutY="53.0" mnemonicParsing="false" onAction="#assignCategory" prefHeight="39.0" prefWidth="191.0" text="Add New Category">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
